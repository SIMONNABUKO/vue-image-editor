{
    "name": "vue-image-editor",
    "version": "1.0.0",
    "description": "A comprehensive Vue.js image editor component with blur, pixelate, text, and emoji features",
    "main": "dist/vue-image-editor.umd.js",
    "module": "dist/vue-image-editor.es.js",
    "files": [
      "dist"
    ],
    "exports": {
      ".": {
        "import": "./dist/vue-image-editor.es.js",
        "require": "./dist/vue-image-editor.umd.js"
      }
    },
    "scripts": {
      "build": "vite build && vue-tsc --emitDeclarationOnly"
    },
    "keywords": [
      "vue",
      "image",
      "editor",
      "blur",
      "pixelate",
      "text",
      "emoji"
    ],
    "author": "Simon Angatia",
    "license": "MIT",
    "peerDependencies": {
      "vue": "^3.0.0"
    },
    "devDependencies": {
      "@vitejs/plugin-vue": "^4.1.0",
      "vite": "^4.3.9",
      "vue": "^3.3.4",
      "vue-tsc": "^1.6.5"
    },
    "types": "dist/types/index.d.ts"
  }
  
  